<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.model.database">

    <class name="com.model.database.onebusaway.gtfs.hibernate.objects.ext.AgencyExt" table="gtfs_agencies">
        <id name="id" type="string" length="50" />
        <property name="fareUrl" />        
        <property name="lang" />
        <property name="name" />
        <property name="phone" />
        <property name="timezone" />
        <property name="url" />
        <property name="defaultId" type="string" length="50" />
        <property name="added" type="string" length="20" />
    </class>

    <class name="com.model.database.onebusaway.gtfs.hibernate.objects.ext.FareAttributeExt" table="gtfs_fare_attributes">
        <composite-id name="id" class="org.onebusaway.gtfs.model.AgencyAndId">
            <key-property name="agencyId" length="50" />
            <key-property name="id" />
        </composite-id>
        <property name="price" />
        <property name="currencyType" />
        <property name="paymentMethod" />
        <property name="transfers" />
        <property name="transferDuration" />
        <property name="journeyDuration" />
    </class>
    
    <class name="com.model.database.onebusaway.gtfs.hibernate.objects.ext.StopServiceMap" table="gtfs_stop_service_map">
        <id name="id" column="gid" type="int" length="11" unsaved-value="null">
            <generator class="native" />
        </id>
        <property name="agencyId" />
        <many-to-one name="stop" not-null="true">
            <column name="agencyId_def" />
            <column name="stopId" />                        
        </many-to-one>                
    </class> 
   
    <class name="com.model.database.onebusaway.gtfs.hibernate.objects.ext.RouteServiceIdMap" table="gtfs_route_serviceid_map">
        <id name="id" column="gid" type="int" length="11" unsaved-value="null">
            <generator class="native" />
        </id>
        <many-to-one name="route" not-null="true" >
            <column name="agencyId" />
            <column name="routeId" />                        
        </many-to-one>        
        <many-to-one name="calendar" not-null="true">
            <column name="agencyId_def" />
            <column name="serviceId" />                        
        </many-to-one>
      	<!--  <property name="agencyId_def" />
       	<property name="serviceId" />  -->                
    </class>

	<class name="com.model.database.onebusaway.gtfs.hibernate.objects.ext.StopRouteMap" table="gtfs_stop_route_map">
        <id name="id" column="gid" type="int" length="11" unsaved-value="null">
            <generator class="native" />
        </id>        
        <property name="agencyId" />
        <property name="routeId" />       
        <many-to-one name="stop" not-null="true">
            <column name="agencyId_def" />
            <column name="stopId" />                        
        </many-to-one>                
    </class>
    
    <class name="com.model.database.onebusaway.gtfs.hibernate.objects.ext.FareRuleExt" table="gtfs_fare_rules">
        <id name="id" column="gid" type="int" length="11" unsaved-value="null">
            <generator class="native" />
        </id>
        <many-to-one name="fare" not-null="true">
            <column name="fare_agencyId" length="50" />
            <column name="fare_id" />
        </many-to-one>
        <many-to-one name="route" not-null="true">
            <column name="route_agencyId" length="50" />
            <column name="route_id" />
        </many-to-one>
        <property name="originId" />
        <property name="destinationId" />
        <property name="containsId" />
    </class>
    
    <class name="com.model.database.onebusaway.gtfs.hibernate.objects.ext.FeedInfoExt" table="gtfs_feed_info">
        <id name="id" column="gid" type="int" length="11" unsaved-value="null">
            <generator class="native" />
        </id>        
        <property name="publisherName" />
        <property name="publisherUrl" />
        <property name="lang" />         
        <property name="startDate" type="org.onebusaway.gtfs.impl.ServiceDateUserType" length="8"/>
        <property name="endDate" type="org.onebusaway.gtfs.impl.ServiceDateUserType" length="8"/>
        <property name="version" />
        <property name="defaultId" type="string" length="50" />
        <property name="agencyIds" type="text"/>
        <property name="agencyNames" type="text"/>
        <property name="feedName" type="text"/>
    </class>

    <class name="com.model.database.onebusaway.gtfs.hibernate.objects.ext.FrequencyExt" table="gtfs_frequencies">
        <id name="id" column="gid" type="int" length="11" unsaved-value="null">
            <generator class="native" />
        </id>
        <many-to-one name="trip" not-null="true">
            <column name="trip_agencyId" length="50" />
            <column name="trip_id" />
        </many-to-one>
        <property name="startTime" />
        <property name="endTime" />
        <property name="exactTimes" />
        <property name="headwaySecs" />
        <property name="labelOnly" />
        <property name="defaultId" type="string" length="50" />
    </class>

    <class name="com.model.database.onebusaway.gtfs.hibernate.objects.ext.PathwayExt" table="gtfs_pathways">
        <composite-id name="id" class="org.onebusaway.gtfs.model.AgencyAndId">
            <key-property name="agencyId" length="50" />
            <key-property name="id" />
        </composite-id>
        <many-to-one name="fromStop" not-null="true">
            <column name="fromStop_agencyId" length="50" />
            <column name="fromStop_id" />
        </many-to-one>
        <many-to-one name="toStop" not-null="true">
            <column name="toStop_agencyId" length="50" />
            <column name="toStop_id" />
        </many-to-one>
        <property name="traversalTime" />
        <property name="wheelchairTraversalTime" />
    </class>
    
    <class name="com.model.database.onebusaway.gtfs.hibernate.objects.ext.RouteExt" table="gtfs_routes">
        <composite-id name="id" class="org.onebusaway.gtfs.model.AgencyAndId">
            <key-property name="agencyId" length="50" />
            <key-property name="id" />
        </composite-id>
        <property name="desc" column="description" type="text"/>
        <property name="shortName" />
        <property name="longName" />
        <property name="type" />
        <property name="url" />
        <property name="color" />
        <property name="textColor" />
        <many-to-one name="agency" not-null="true" />
        <property name="defaultId" type="string" length="50" />
    </class>

    <class name="com.model.database.onebusaway.gtfs.hibernate.objects.ext.ServiceCalendarExt" table="gtfs_calendars">
        <!--  <id name="id" column="gid" type="int" length="11" unsaved-value="null">
            <generator class="native" />
        </id> -->
        <composite-id name="serviceId" class="org.onebusaway.gtfs.model.AgencyAndId">
            <key-property name="agencyId" column="serviceId_agencyId" length="50" />
            <key-property name="id" column="serviceId_id" />
        </composite-id>
        <property name="id" column="gid" type="int" length="11"/>
        <property name="monday" />
        <property name="tuesday" />
        <property name="wednesday" />
        <property name="thursday" />
        <property name="friday" />
        <property name="saturday" />
        <property name="sunday" />
        <property name="startDate" type="org.onebusaway.gtfs.impl.ServiceDateUserType" length="8"/>
        <property name="endDate" type="org.onebusaway.gtfs.impl.ServiceDateUserType" length="8"/>        
    </class>

    <class name="com.model.database.onebusaway.gtfs.hibernate.objects.ext.ServiceCalendarDateExt" table="gtfs_calendar_dates">
        <id name="id" column="gid" type="int" length="11" unsaved-value="null">
            <generator class="native" />
        </id>
        <property name="date" type="org.onebusaway.gtfs.impl.ServiceDateUserType" length="8"/>
        <property name="exceptionType" />
        <component name="serviceId" class="org.onebusaway.gtfs.model.AgencyAndId">
            <property name="agencyId" column="serviceId_agencyId" index="serviceId" length="50" />
            <property name="id" column="serviceId_id" index="serviceId" />
        </component>
    </class>

    <class name="com.model.database.onebusaway.gtfs.hibernate.objects.ext.ShapePointExt" table="gtfs_shape_points">
        <id name="id" column="gid" type="int" length="11" unsaved-value="null">
            <generator class="native" />
        </id>
        <property name="sequence" />
        <property name="distTraveled" />
        <property name="lat" />
        <property name="lon" />
        <component name="shapeId" class="org.onebusaway.gtfs.model.AgencyAndId">
            <property name="agencyId" column="shapeId_agencyId" index="shapeId" length="50" />
            <property name="id" column="shapeId_id" index="shapeId" />
        </component>
    </class>

    <class name="com.model.database.onebusaway.gtfs.hibernate.objects.ext.StopExt" table="gtfs_stops">
        <composite-id name="id" class="org.onebusaway.gtfs.model.AgencyAndId">
            <key-property name="agencyId" length="50" />
            <key-property name="id" />
        </composite-id>
        <property name="code" />
        <property name="name" />
        <property name="desc" column="description" />
        <property name="direction" />
        <property name="lat" />
        <property name="lon" />
        <property name="zoneId" />
        <property name="url" />
        <property name="locationType" />
        <property name="parentStation" />
        <property name="timezone" />
        <property name="vehicleType" />
        <property name="wheelchairBoarding" />
        <property name="platformCode" />
        <property name="blockId" />        
        <property name="placeId" />
        <property name="regionId" />
        <property name="urbanId" />
        <property name="congdistId" />
    </class>

    <class name="com.model.database.onebusaway.gtfs.hibernate.objects.ext.StopTimeExt" table="gtfs_stop_times">
        <id name="id" column="gid" type="int" length="11" unsaved-value="null">
            <generator class="native" />
        </id>
        <many-to-one name="stop" not-null="true">
            <column name="stop_agencyId" length="50" />
            <column name="stop_id" />
        </many-to-one>
        <many-to-one name="trip" not-null="true">
            <column name="trip_agencyId" length="50" index="stop_time_trip_agency_id_idx" />
            <column name="trip_id" index="stop_time_trip_id_idx" />
        </many-to-one>

        <property name="stopSequence" />
        <property name="arrivalTime" />
        <property name="departureTime" />
        <property name="timepoint"/>
        <property name="stopHeadsign" />
        <property name="routeShortName" />
        <property name="pickupType" />
        <property name="dropOffType" />
        <property name="shapeDistTraveled" />

    </class>

    <class name="com.model.database.onebusaway.gtfs.hibernate.objects.ext.TransferExt" table="gtfs_transfers">
        <id name="id" column="gid" type="int" length="11" unsaved-value="null">
            <generator class="native" />
        </id>
        <many-to-one name="fromStop" not-null="true">
            <column name="fromStop_agencyId" length="50" />
            <column name="fromStop_id" />
        </many-to-one>
        <many-to-one name="fromRoute" not-null="true">
            <column name="fromRoute_agencyId" length="50" />
            <column name="fromRoute_id" />
        </many-to-one>
        <many-to-one name="fromTrip" not-null="true">
            <column name="fromTrip_agencyId" length="50" />
            <column name="fromTrip_id" />
        </many-to-one>
        <many-to-one name="toStop" not-null="true">
            <column name="toStop_agencyId" length="50" />
            <column name="toStop_id" />
        </many-to-one>
        <many-to-one name="toRoute" not-null="true">
            <column name="toRoute_agencyId" length="50" />
            <column name="toRoute_id" />
        </many-to-one>
        <many-to-one name="toTrip" not-null="true">
            <column name="toTrip_agencyId" length="50" />
            <column name="toTrip_id" />
        </many-to-one>
        <property name="transferType" />
        <property name="minTransferTime" />
        <property name="defaultId" type="string" length="50" />
    </class>
  
    <class name="com.model.database.onebusaway.gtfs.hibernate.objects.ext.TripExt" table="gtfs_trips">

        <composite-id name="id" class="org.onebusaway.gtfs.model.AgencyAndId">
            <key-property name="agencyId" length="50" />
            <key-property name="id" />
        </composite-id>

        <many-to-one name="route" not-null="true">
            <column name="route_agencyId" length="50" />
            <column name="route_id" />
        </many-to-one>

        <component name="serviceId" class="org.onebusaway.gtfs.model.AgencyAndId">
            <property name="agencyId" column="serviceId_agencyId" index="serviceId" length="50" />
            <property name="id" column="serviceId_id" index="serviceId" />
        </component>

        <component name="shapeId" class="org.onebusaway.gtfs.model.AgencyAndId">
            <property name="agencyId" column="shapeId_agencyId" length="50" />
            <property name="id" column="shapeId_id" />
        </component>

        <property name="tripShortName" />
        <property name="tripHeadsign" />
        <property name="routeShortName" />
        <property name="directionId" />
        <property name="blockId" index="blockId" />
        <property name="wheelchairAccessible"/>
        <property name="epshape" type="org.hibernate.type.TextType" column="epshape"/>
        <property name="length"/>
        <property name="estlength"/>
        <property name="tlength"/>
        <property name="stopscount"/>
        <property name="uid"/>
    </class>
	

</hibernate-mapping>
